program: 
    | haiToken eolToken statement eolToken byeToken
	;

statement: expression
    | statement eolToken statement 
    ;

expression:
    | mathOp
    | comment
    | vardec
    ;

mathOp: literal
	| sumOpToken mathOp AN mathOp
	| diffOpToken mathOp AN mathOp
	| mulOpToken mathOp AN mathOp
	| divOpToken mathOp AN mathOp
	| minOpToken mathOp AN mathOp
	| maxOpToken mathOp AN mathOp
	;

comment:
	| obtwToken statement tldrToken
	;
	
vardec: 
	| ihasToken idToken
	| ihasToken idToken itzToken literal
	| ihasToken idToken itzToken mathOp
	| ihasToken idToken itzToken idToken
	;
	
literal:
	| numbrToken
	| numbarToken
	| yarnToken
	| troofToken
	;
	